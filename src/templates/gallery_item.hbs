{{#each this}}
  <a class='gallery__link' href={{largeImageURL}}>
    <div class='photo-card'>
      <img
        src={{webformatURL}}
        alt='{{tags}}'
        loading='lazy'
        width='320'
        height='195'
      />
      <div class='info'>
        <p class='info-item'>{{likes}}
          <b> Likes</b>
        </p>
        <p class='info-item'>{{views}}
          <b> Views</b>
        </p>
        <p class='info-item'>{{comments}}
          <b> Comments</b>
        </p>
        <p class='info-item'>{{downloads}}
          <b> Downloads</b>
        </p>
      </div>
    </div>
  </a>
{{/each}}